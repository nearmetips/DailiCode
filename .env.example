# OpenRouter Configuration (推荐使用)
# 获取 API Key: https://openrouter.ai/keys
USE_CUSTOM_LLM=true
CUSTOM_LLM_PROVIDER="openai"
CUSTOM_LLM_API_KEY="your-openrouter-api-key-here"
CUSTOM_LLM_ENDPOINT="https://openrouter.ai/api/v1"
CUSTOM_LLM_MODEL_NAME="anthropic/claude-3.5-sonnet"

# 可选参数
CUSTOM_LLM_TEMPERATURE=0.7
CUSTOM_LLM_MAX_TOKENS=8192
CUSTOM_LLM_TOP_P=1

